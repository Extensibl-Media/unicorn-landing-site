---
import AdminLayout from "@/layouts/AdminLayout.astro";
import { createAdminClient } from "@/lib/supabase/admin";
import LinksTable from "@/components/admin/links-table.tsx";

const client = createAdminClient();

const { data: linksData } = await client
    .from("lib_links")
    .select("*")
    .order("order", { ascending: true });

// console.log({ linksData });
---

<AdminLayout title="Links">
    <div class="flex flex-col gap-4">
        <div>
            <h1 class="text-2xl font-bold tracking-tight">Links</h1>
            <p class="text-muted-foreground">
                View and manage Link In Bio links.
            </p>
        </div>
        <LinksTable links={[]} client:load />
    </div>
</AdminLayout>
